<body id="admission_bg">
	<div id="form_container" class="clearfix">
		<figure>
			<a href="/"><img src="../dini-akademi-logo.png" width="130" height="60" data-retina="true" alt=""></a>
		</figure>
		<div id="wizard_container">
			<div id="top-wizard">
				<div id="progressbar"></div>
			</div>
			<!-- /top-wizard -->
			<form action="/users/register" method="POST">
				<input id="website" name="website" type="text" value="">
				<input type="hidden" name="tip" value="2">
				<!-- Leave for security protection, read docs for details -->
				<div id="middle-wizard">
					<div class="step">
						<div id="intro">
							<h1>Dini Akademi Online Eğitim Platformu</h1>
							<p>Sistemimize üye olun ve ayrıcalıklardan faydalanmaya hemen başlayın!</p>

						</div>
					</div>

					<div class="step">
						<h3 class="main_question"><strong>1/3</strong>Aşağıdaki alanları doldurunuz.</h3>
						<div class="form-group">
							<input type="text" name="ad"  class="form-control required sadeceharf" pattern="[A-Za-z]{3}" placeholder="Adınız">
						</div>
						<div class="form-group">
							<input type="text" name="soyad"  class="form-control required sadeceharf" placeholder="Soyadınız">
						</div>
						<div class="form-group">
							<input type="number" name="tc" onkeypress="return isNumberKey(event)" id="myinput2" maxlength="12" class="form-control required" placeholder="TC Kimlik No">
						</div>
						<div class="form-group">
							<input type="text" name="tel" onkeypress="return isNumberKey(event)" class="form-control required" placeholder="Telefon Numaranız">
						</div>
						<div class="form-group">
							<input type="text" name="okul" class="form-control required" placeholder="Okulunuz">
						</div>
						<div class="form-group">
							<input type="number" name="yas" onkeypress="return isNumberKey(event)" class="form-control required" placeholder="Yaşınız">
						</div>
						<div class="form-group radio_input">
							<label><input type="radio" value="Erkek" checked name="cinsiyet" class="icheck"> Erkek</label>
							<label><input type="radio" value="Kadın" name="cinsiyet" class="icheck"> Kadın</label>
						</div>
					</div>
					<!-- /step-->

					<div class="step" id="app">
						<h3 class="main_question"><strong>2/3</strong>Aşağıdaki alanları doldurunuz.</h3>
						<div class="form-group">
							<select name="sehir" class="form-control" v-model="selected">
								<option value="*" selected="selected"></option>
								<option v-for="item in sehir" :value="item.il">{{item.il}}</option>
							</select>
						</div>
						<div class="form-group">
							<select class="form-control required" name="ilce" v-for="item in sehir" v-model="ilcee" v-if="item.il == selected">
								<option v-for="ilce in item.ilceleri" :value="ilce">{{ilce}} </option>
							</select>
						</div>
						<div class="form-group">
							<input type="text" name="adres" class="form-control required" placeholder="Adres">
						</div>
					</div>
					<!-- /step-->

					<div class="submit step">
						<h3 class="main_question"><strong>3/3</strong>İşlemleri tamamlayın</h3>
						<div class="form-group">
							<input type="email" class="form-control required" name="username" placeholder="Mail Adresinizi giriniz">
						</div>
						<div class="form-group">
							<input type="password" class="form-control required" name="password" placeholder="Şifrenizi giriniz">
						</div>
						<div class="form-group terms">
							<input name="terms" type="checkbox" class="icheck required" value="yes">
							<label>Okudum, onaylıyorum. <a data-toggle="modal" data-target="#myModal">Kullanıcı sözleşmesi.</a></label>
						</div>
						<div class="col-md-12">
							<button type="submit" class="btn btn-primary" formaction="/users/register">KAYIT OL</button>
						</div>
					</div>
					<!-- /step-->
				</div>
				<!-- /middle-wizard -->
				<div id="bottom-wizard">
					<button type="button" name="backward" class="backward">Geri </button>
					<button type="button" name="forward" class="forward">İleri</button>
				</div>
				<!-- /bottom-wizard -->
			</form>
		</div>
		<!-- /Wizard container -->
	</div>
	<!-- /Form_container -->

	<!-- /.modal -->
	<div id="myModal" class="modal fade" role="dialog" style="padding-top: 100px">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Şartlar ve Koşullar</h4>
      </div>
      <div class="modal-body" style="height: 500px">
        <iframe src="/sartlar"></iframe>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Çıkış</button>
      </div>
    </div>

  </div>
</div>
	<script type="text/javascript">
		$(document).ready(function () {
		    $(".sadeceharf").keypress(function (evt) {
		             var charCode = (evt.which) ? evt.which : event.keyCode
		            //Bu şartımız ile harf girildiği takdirde true olarak geri dönüş sağlıyoruz. 
		            //(Yalnızca İngilizce karakterleri desteklemektedir. Türkçe karakterler için 2. Yöntemi kullanabilirsiniz.)
		            if ((charCode <=93 && charCode >=65) ||(charCode <=122 && charCode >=97) || charCode==8) {
		                return true;
		            }
		            return false;
		        });
		 });

		function isNumberKey(evt) {
		    var charCode = (evt.which) ? evt.which : event.keyCode;
		    if (charCode > 31 && (charCode < 48 || charCode > 57))
		        return false;
		    return true;
		}
	</script>

